{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "https://chatgpt.com/",
    "distDir": "../dist"
  },
  "package": {
    "productName": "ChatGPT Tray",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "category": "Productivity",
      "identifier": "com.openai.chatgpt.tray"
    },
    "security": {
      "csp": null
    },
    "systemTray": {
      "iconAsTemplate": true
    },
    "windows": [
      {
        "label": "main",
        "title": "ChatGPT",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "visible": false,
        "url": "https://chatgpt.com/"
      }
    ]
  }
}
